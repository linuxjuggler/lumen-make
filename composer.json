{
    "name": "damascene/lumen-make",
    "description": "Adding few missing commands",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zaher Ghaibeh",
            "email": "z@zah.me"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.6.*"        
    },
    "require-dev": {
        "pds/skeleton": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Damascene\\Commands\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "fix" : "php vendor/bin/php-cs-fixer fix --using-cache=no"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }    
}
